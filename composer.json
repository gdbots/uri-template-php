{
  "name": "gdbots/uri-template",
  "description": "UriTemplate expander for php and a library of common uri templates.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3",
    "guzzle/guzzle": "~3.7"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  },
  "autoload": {
    "files": ["uri-templates.php"],
    "psr-4": {
      "Gdbots\\UriTemplate\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\UriTemplate\\": "tests"
    }
  }
}
